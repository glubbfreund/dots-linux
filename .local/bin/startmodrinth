#!/bin/bash
# ~/.local/bin/startmodrinth

gpumode=$(~/.local/bin/gpumonitor)
if [[ "$gpumode" == "Opt" ]];then
    echo "Optimus, lade..."
    __NV_PRIME_RENDER_OFFLOAD=1 __GLX_VENDOR_LIBRARY_NAME=nvidia ~/Dokumente/AppImage/ModrinthApp_0.10.3_amd64.AppImage "$@" &
else
    echo "Ultimate, lade.."
    ~/Dokumente/AppImage/ModrinthApp_0.10.3_amd64.AppImage "$@" &
fi
