#!/bin/bash
# ~/.local/bin/gpu_int

[ "$(supergfxctl -g)" = "AsusMuxDgpu" ] && yad --text="Auf integrierte GPU umschalten und neu starten?" && supergfxctl -m Integrated && xfce4-session-logout --reboot || [ $(supergfxctl -g) != "Integrated" ] && yad --text="Auf integrierte GPU umschalten?" && ([ "$(supergfxctl -g)" != "AsusMuxDgpu" ] && supergfxctl -m Integrated && xfce4-session-logout --logout)
