#!/bin/bash
# ~/.local/bin/gpu_integ

[ "$(supergfxctl -g)" = "AsusMuxDgpu" ] && yad --text="Auf Hybridmodus umschalten und neu starten?" && supergfxctl -m Hybrid && xfce4-session-logout --reboot || ([ "$(supergfxctl -g)" != "AsusMuxDgpu" ] && supergfxctl -m Hybrid && xfce4-session-logout --logout)
